{
    "name": "the-coding-owl/be-user-notes",
    "description": "This extension provides an extension to the sys_note records and their functionality accross the TYPO3 backend. It provides a toolbar item that shows unread notes and a module to create and send notes to other users",
    "type": "typo3-cms-extension",
    "require": {
        "typo3/cms-core": "7.6.* || 8.7.*",
        "typo3/cms-backend": "7.6.* || 8.7.*",
        "typo3/cms-lang": "7.6.* || 8.7.*",
        "typo3/cms-sys-note": "7.6.* || 8.7.*"
    },
    "license": "GPLv3",
    "authors": [
        {
            "name": "Kevin Ditscheid",
            "email": "kevinditscheid@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TheCondingOwl\\BeUserNotes\\": "Classes/"
        }
    },
    "replace": {
        "be_user_notes": "self.version",
        "typo3-ter/be-user-notes": "self.version"
    }
}
